{
  "manifest_version": 2,

  "name": "WheePin",
  "description": "Saved blogs",
  "version": "0.1",

  "browser_action": {
    "_comment": "icon.png is provided by DesignModo under the Creative Commons License",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png"
	},
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}
